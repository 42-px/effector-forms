{
  "presets": [
    "@babel/preset-typescript",
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "chrome": "70",
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "effector/babel-plugin",
    [
      "module-resolver",
      {
        "alias": {
          "effector-forms": "effector-forms/ssr"
        }
      }
    ],
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}